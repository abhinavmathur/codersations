<% if current_user.try(:creator) && (current_user == User.find(params[:id]))%>
    <%= render partial: 'creator_page', locals: { user: @user} %>
<% elsif current_user.try(:manager) %>
<!-- TODO-->
<% else %>
    <!-- This is the view when someone else views the website(not current user)-->
    <%= render partial: 'normal_user',
               locals: { tutorials: @tutorials, snippets: @snippets, templates: @templates, user: @user } %>
<% end %>


